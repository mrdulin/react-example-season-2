{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "helpers": true,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],
    "transform-decorators-legacy",
    "add-module-exports",
    [
      "module-resolver",
      {
        "root": [
          "./app"
        ],
        "alias": {
          "app": "./app"
        }
      }
    ]
  ]
}
