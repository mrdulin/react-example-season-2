  "scripts": {
    "start": "nodemon --watch webpack.config.js --delay 1000ms --exec webpack-dev-server --progress --hot",
    "serve": "NODE_ENV=node-server-proxy webpack-dev-server --progress --hot",
    "build": "webpack --progress",
    "publish": "npm run dll && cross-env NODE_ENV=production webpack --progress --hide-modules && git pull && git add . && git commit -m 'publish' && git push &&  git push --tags",
    "dll": "cross-env NODE_ENV=development webpack --config webpack.dll.js --progress",
    "watch": "webpack --watch --progress",
    "stats": "webpack --profile --json --progress > stats.json",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "verbose": true,
    "bail": true,
    "modulePaths": [
      "<rootDir>/app/"
    ],
    "notify": true,
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)(spec|test).js?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/static/",
      "<rootDir>/dist/",
      "<rootDir>/node_modules/"
    ]
  },
  "keywords": [
    "react",
    "react-dom",
    "react-lazy-load",
    "code-split",
    "webpack",
    "babel",
    "react-router",
